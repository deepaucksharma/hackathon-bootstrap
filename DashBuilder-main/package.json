{
  "name": "dashbuilder",
  "version": "3.0.0",
  "description": "Unified New Relic Dashboard Builder - Create intelligent dashboards with ease",
  "main": "src/index.js",
  "bin": {
    "dashbuilder": "./cli/dashbuilder.js"
  },
  "scripts": {
    "start": "node cli/dashbuilder.js",
    "test": "jest",
    "lint": "eslint src/ cli/",
    "format": "prettier --write \"src/**/*.js\" \"cli/**/*.js\"",
    "validate": "npm run lint && npm test",
    "example:kafka": "node examples/kafka-monitoring.js",
    "example:basic": "node examples/basic-dashboard.js"
  },
  "keywords": [
    "newrelic",
    "dashboard",
    "monitoring",
    "observability",
    "nrql",
    "metrics",
    "kafka",
    "automation"
  ],
  "author": "DashBuilder Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.7",
    "bottleneck": "^2.19.5",
    "chalk": "^4.1.2",
    "commander": "^12.1.0",
    "dotenv": "^16.5.0",
    "inquirer": "^8.2.6",
    "joi": "^17.12.2",
    "lru-cache": "^10.4.3",
    "ora": "^5.4.1",
    "p-limit": "^3.1.0",
    "winston": "^3.11.0",
    "ws": "^8.18.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.5"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/dashbuilder.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/dashbuilder/issues"
  },
  "homepage": "https://github.com/your-org/dashbuilder#readme"
}

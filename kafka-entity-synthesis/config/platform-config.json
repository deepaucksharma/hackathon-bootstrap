{
  "newRelic": {
    "accountId": "${ACC}",
    "apiKeys": {
      "insert": "${IKEY}",
      "query": "${QKey}",
      "user": "${UKEY}"
    },
    "endpoints": {
      "events": "https://insights-collector.newrelic.com/v1/accounts/{accountId}/events",
      "graphql": "https://api.newrelic.com/graphql",
      "nrql": "https://api.newrelic.com/graphql"
    }
  },
  "aws": {
    "accountId": "${AWS_ACCOUNT_ID}",
    "region": "us-east-1"
  },
  "testing": {
    "defaultTimeout": 30000,
    "retryAttempts": 3,
    "verificationDelay": 5000
  },
  "logging": {
    "level": "info",
    "outputDir": "results"
  }
}
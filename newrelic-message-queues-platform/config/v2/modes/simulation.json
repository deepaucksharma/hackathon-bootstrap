{
  "scenarios": ["production", "development", "chaos"],
  "defaultScenario": "production",
  "patterns": {
    "businessHours": true,
    "anomalies": true,
    "seasonality": true,
    "specialEvents": false
  },
  "providers": ["kafka", "rabbitmq", "sqs"],
  "entityCounts": {
    "clusters": 2,
    "brokers": 6,
    "topics": 20,
    "queues": 15,
    "consumers": 50,
    "producers": 30
  },
  "metrics": {
    "updateInterval": 30000,
    "jitter": 0.1,
    "outlierProbability": 0.05
  },
  "scenarios": {
    "production": {
      "load": "high",
      "reliability": 0.99,
      "latencyMs": {
        "p50": 10,
        "p95": 50,
        "p99": 100
      }
    },
    "development": {
      "load": "low",
      "reliability": 0.95,
      "latencyMs": {
        "p50": 5,
        "p95": 20,
        "p99": 50
      }
    },
    "chaos": {
      "load": "variable",
      "reliability": 0.85,
      "failures": {
        "broker": 0.1,
        "network": 0.05,
        "consumer": 0.15
      }
    }
  }
}
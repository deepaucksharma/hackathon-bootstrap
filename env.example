# New Relic Configuration
# Copy this file to .env and fill in your actual values

# New Relic License Key (Ingest Key) - starts with NRAL
IKEY=your-license-key-here

# New Relic User API Key - starts with NRAK
UKEY=your-user-api-key-here

# New Relic Query Key - starts with NRIQ (optional)
QKey=your-query-key-here

# New Relic Account ID
ACC=your-account-id-here

# AWS Configuration (optional - for MSK shim)
# AWS_ACCOUNT_ID=123456789012
# AWS_REGION=us-east-1